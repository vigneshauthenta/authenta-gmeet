{
  "manifest_version": 3,
  "name": "Authenta Check",
  "version": "1.0",
  "description": "Detect Video Face Swaps",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "content.js"
  },
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}
